{
  "name": "nextjs-boilerplate",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "generate": "plop --plopfile generators/plopfile.js",
    "test": "jest  --bail --maxWorkers=50% ",
    "test:watch": "jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "postinstall": "husky install",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "next": "^13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-essentials": "7.2.2-alpha.0",
    "@storybook/nextjs": "7.2.2-alpha.0",
    "@storybook/react": "7.2.2-alpha.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.3",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.17",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^10.11.0",
    "eslint": "^7.3.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^7.0.4",
    "jest": "^26.1.0",
    "jest-styled-components": "^7.1.0",
    "plop": "^3.1.1",
    "prettier": "2.6.2",
    "start-server-and-test": "^1.15.2",
    "storybook": "7.2.2-alpha.0",
    "typescript": "^4.9.3",
    "webpack": "5.68.0"
  },
  "overrides": {
    "@mdx-js/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "license": "MIT",
  "author": {
    "name": "Clayton Souza",
    "email": "clatyon.rssouza@gmail.com",
    "url": "https://github.com/Claytonrss"
  },
  "engines": {
    "node": "18"
  }
}
